[target]
target ansible_host=127.0.0.1 ansible_port=2222 ansible_user=vagrant ansible_private_key_file=PATH_TO_PRIVATE_KEY ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'

[monitoring]
monitoring ansible_host=127.0.0.1 ansible_port=2200 ansible_user=vagrant ansible_private_key_file=PATH_TO_PRIVATE_KEY ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'

[all:vars]
ansible_python_interpreter=/usr/bin/python3
